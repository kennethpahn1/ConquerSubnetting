<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kennethpahn.infs1609tutorialapplication">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".moduleDetail" />
        <activity
            android:name=".moduleContentDp"
            android:label="@string/title_activity_module_content_dp"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".contentDisplay"
            android:label="@string/title_activity_content_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".test" />
        <activity android:name=".contentDisp" />
        <activity android:name=".mcqQuizDisp" />
        <activity android:name=".tfQuizDisp" />
        <activity android:name=".multimediaContentDisp" />
        <activity
            android:name=".LaunchActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IntroActivity" />
        <activity android:name=".login" />
        <activity android:name=".register"></activity>
    </application>

</manifest>